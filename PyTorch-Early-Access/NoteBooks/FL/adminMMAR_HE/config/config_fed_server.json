{
    "format_version": 1,

    "servers": [
        {
            "name": "spleen_segmentation",
            "min_num_clients": 2,
            "max_num_clients": 100,
            "wait_after_min_clients": 10,
            "heart_beat_timeout": 600,
            "start_round": 0,
            "num_rounds": 2
        }
    ],
    "aggregator":
      {
        "standard_path": "flare.experimental.homomorphic_encryption.he_accumulate_model_aggregator.HEAccumulateWeightedAggregator",
        "intime_path": "flare.experimental.homomorphic_encryption.he_intime_accumulate_model_aggregator.HEInTimeAccumulateWeightedAggregator",
        "regular_name": "AccumulateWeightedAggregator",
        "path": "flare.experimental.homomorphic_encryption.he_accumulate_model_aggregator.HEAccumulateWeightedAggregator",
        "args": {
            "aggregation_weights":
                {
                "client0": 1,
                "client1": 1.5,
                "client2": 0.8
                }
          }
      },
    "outbound_filters": [
    ],
    "inbound_filters": [
    ],
    "model_persistor":
    {
        "path": "flare.experimental.homomorphic_encryption.he_pt_file_model_persistor.HEPTFileModelPersistor",
        "args": {
          "exclude_vars": "dummy",
          "model": {
            "path": "monai.networks.nets.unet.UNet",
            "args": {
              "dimensions": 3,
              "in_channels": 1,
              "out_channels": 2,
              "channels": [16, 32, 64, 128, 256],
              "strides": [2, 2, 2, 2],
              "num_res_units": 2,
              "norm": "batch"
            }
          }
        }
    },
    "shareable_generator": {
      "path": "flare.experimental.homomorphic_encryption.he_model_shareable_generator.HEModelShareableGenerator",
      "args": {
          "tenseal_context_file": "/workspace/startup/server_context.tenseal"
      }
    }
}
