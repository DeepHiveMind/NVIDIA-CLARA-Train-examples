{
  "format_version": 1,
    "servers": [
        {
            "name": "spleen_segmentation"
        }
    ],
    "client": {
        "outbound_filters": [
          {
            "path": "flare.experimental.homomorphic_encryption.he_model_encryptor.HEModelEncryptor",
            "args": {
              "encrypt_layers": [
                "model.0.conv.unit0.conv.weight",
                "model.1.submodule.1.submodule.1.submodule.2.0.conv.weight",
                "model.2.1.conv.unit0.conv.weight"
              ]
            }
          }
        ],
        "inbound_filters": [
          {
            "path": "flare.experimental.homomorphic_encryption.he_model_decryptor.HEModelDecryptor"
          }
        ]
    },
  "client_trainer": {
    "path": "medl.apps.fed_learn.trainers.client_trainer.ClientTrainer",
    "args": {
      "local_epochs": 2,
        "steps_aggregation": 0,
        "model_reader_writer": {
            "name": "PTModelReaderWriter"
        }
    }
  },
  "handlers": [
  ],
  "cross_validate": true,
  "cross_site_validation": {
    "is_participating": true,
    "model_validator": {
      "path": "medl.apps.fed_learn.validators.client_validator.ClientValidator",
      "args": {}
    },
    "inbound_filters": [
      {
        "path": "medl.apps.fed_learn.filters.he_eval_decryptor.HEEvalDecryptor",
        "args": {
        }
      }
    ],
    "outbound_filters": []
  }
}
