{
    "format_version": 1,
    "servers": [
        {
            "name": "spleen_segmentation",
            "min_num_clients": 2,
            "max_num_clients": 100,
            "wait_after_min_clients": 10,
            "heart_beat_timeout": 600,
            "start_round": 0,
            "num_rounds": 2
        }
    ],
    "aggregator":
      {
        "name": "InTimeAccumulateWeightedAggregator",
        "args": {
          "aggregation_weights":
              {
                "client0": 1,
                "client1": 1.5,
                "client2": 0.8
              }
        }
      },
    "outbound_filters": [
    ],
    "inbound_filters": [
    ],
    "model_persistor":
    {
        "name": "PTFileModelPersistor",
        "args": {
          "exclude_vars": "dummy",
          "model": {
        "path": "monai.networks.nets.unet.UNet",
            "args": {
              "dimensions": 3,
              "in_channels": 1,
              "out_channels": 2,
              "channels": [16, 32, 64, 128, 256],
              "strides": [2, 2, 2, 2],
              "num_res_units": 2,
              "norm": "batch"
            }
          }
        }
    },
    "shareable_generator": {
      "name": "FullModelShareableGenerator"
    },
  "handlers": [
    {
      "path": "nvflare.components.handlers.intime_model_selection_handler.IntimeModelSelectionHandler",
      "args": {}
    }
  ]
}
