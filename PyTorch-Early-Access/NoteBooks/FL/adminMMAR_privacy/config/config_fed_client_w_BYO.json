{
  "format_version": 1,

    "servers": [
        {
            "name": "spleen_segmentation"
        }
    ],
    "client": {
        "outbound_filters": [
          {
              "path": "medl.apps.fed_learn.client.metadata_generator.IteratorNumberGenerator",
              "args": {
                  "field": "n_iter"
              }
          },
          {
              "path": "medl.apps.fed_learn.client.metadata_generator.BestMetricGenerator",
              "args": {
                  "field": "best_validation_metric"
              }
          },
          {
              "path": "flare.components.filters.percentile_privacy.PercentilePrivacy",
              "args": {
                  "gamma": 0.1
              }
          },
          {
              "name2": "LaplacianPrivacy",
              "path2": "flare.components.filters.percentile_privacy.LaplacianPrivacy",
              "change_args": {
                  "gamma": 0.1
              }
          },
          {
              "name3": "SVTPrivacy",
              "path_maybe?3": "flare.components.filters.svt_privacy.SVTPrivacy",
              "change_args": {
                  "gamma": 0.1
              }
          },
          {"usecase_A":"compess"}
        ],
        "inbound_filters": [
          {"usecase_A":"de-compess "}
        ]
    },
  "client_trainer": {
    "path": "medl.apps.fed_learn.trainers.client_trainer.ClientTrainer",
    "path2": "BYOC.MyClientTrainer",
    "args": {
        "local_epochs": 5,
        "steps_aggregation": 0,
        "model_reader_writer": {
            "name": "PTModelReaderWriter"
        }
    }
  },
  "handlers": [
  ],
  "model_validator": {
    }
}
