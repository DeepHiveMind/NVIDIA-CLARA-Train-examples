{
  "format_version": 1,

    "servers": [
        {
            "name": "spleen_segmentation"
        }
    ],
    "client": {
        "outbound_filters": [
          {
              "path": "medl.apps.fed_learn.client.metadata_generator.IteratorNumberGenerator",
              "args": {
                  "field": "n_iter"
              }
          },
          {
              "path": "medl.apps.fed_learn.client.metadata_generator.BestMetricGenerator",
              "args": {
                  "field": "best_validation_metric"
              }
          },
          {
              "name": "PercentilePrivacy",
              "args": {
                 "percentile": 75,
                 "gamma": 50
              }
          },
          {"usecase_A":"compess"}
        ],
        "inbound_filters": [
          {"usecase_A":"de-compess "}
        ]
    },
  "client_trainer": {
    "path": "medl.apps.fed_learn.trainers.client_trainer.ClientTrainer",
    "path2": "BYOC.MyClientTrainer",
    "args": {
        "local_epochs": 5,
        "steps_aggregation": 0,
        "model_reader_writer": {
            "name": "PTModelReaderWriter"
        }
    }
  },
  "handlers": [
  ],
  "model_validator": {
    }
}
